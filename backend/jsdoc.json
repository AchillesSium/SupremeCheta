{
  "source": {
    "include": ["src", "server.js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark",
      "title": "Supreme Cheta API Documentation",
      "menu": [
        {
          "title": "GitHub",
          "link": "https://github.com/yourusername/supreme-cheta",
          "target": "_blank"
        }
      ]
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  }
}
